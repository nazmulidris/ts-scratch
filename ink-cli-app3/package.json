{
  "name": "ink-cli-app3",
  "version": "0.0.1",
  "license": "Apache 2.0",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start-dev": "node -r tsm src/cli.tsx",
    "start-dev-watch": "nodemon --exitcrash -e ts,tsx --exec 'node -r tsm src/cli.tsx || exit 1'",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "eslint '**/src/**/*.{ts,tsx}' --quiet --fix",
    "build": "rm -rf dist/ && tsc && chmod +x dist/cli.js",
    "start": "node dist/cli.js",
    "start-watch-js": "nodemon --exitcrash -e ts,tsx,js,jsx --exec 'node dist/cli.js || exit 1'",
    "build-watch-js": "tsc --watch",
    "dev-js": "npm run build && npm-run-all -p -r build-watch-js start-watch-js"
  },
  "files": [
    "dist/cli.js"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "chalk": "latest",
    "commander": "^8.3.0",
    "ink": "^3.2.0",
    "r3bl-ts-utils": "latest",
    "react": "^17.0.2",
    "react-redux": "^7.2.6",
    "tslib": "^2.3.1",
    "lodash": "latest"
  },
  "devDependencies": {
    "typescript": "^4.4.4",
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "ink-testing-library": "^2.1.0",
    "eslint": "latest",
    "eslint-loader": "latest",
    "@typescript-eslint/parser": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "jest": "latest",
    "nodemon": "latest",
    "npm-run-all": "latest",
    "prettier": "latest",
    "ts-jest": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "tsm": "latest"
  }
}
